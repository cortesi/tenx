#!/bin/sh
set -e

SCRIPT_DIR=$( cd -- "$( dirname -- "${BASH_SOURCE[0]}" )" &> /dev/null && pwd )

echo $SCRIPT_DIR
cd $SCRIPT_DIR/..

cargo run -- new
cargo run -- edit \
    -vvv \
    --prompt "Please complete the fibonacci function" \
    $SCRIPT_DIR/simple/src/*.rs 
git diff samples/simple
git checkout samples/simple
